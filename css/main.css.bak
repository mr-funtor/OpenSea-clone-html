*{
	box-sizing:border-box;
	margin:0;
	padding:0;
}

:root{
	--logo-color:#1868b7;
	--accent-color:rgb(32 129 226);
	--text-color:#000;
	--light-text-color:rgb(112, 122, 131);
	--sub-text-color:rgba(4, 17, 29, 0.75);
	--main-border-rad:12px;
	--gen-shadow:2px 2px 3px 4px rgba(169, 169, 169,0.17),-2px -2px 3px 4px rgba(169, 169, 169,0.17);
}

body{
	font-family: Poppins, sans-serif;
}

a{
	text-decoration:none;
	color:black;
}

	/* NAV BAR */
.nav-bar{
	position:sticky;
	top:0;
	height: 73px;
	background: #fff;
	padding: 8px 25px;
	display:grid;
	grid-template-columns: 1.6fr 2fr 2fr 0.5fr;
	align-items:center;
	box-shadow: 0px 2px 2px rgba(211,211,211,1);
	z-index:99;
}

.logo-area{
	display:flex;
	align-items:center;
	/* background: blue; */
	cursor:pointer;
}

.logo{
	height: 6.5vh;
	padding-right: 10px;
}

.search-area{
	height: 45px;
	padding: 15px;
	max-width: 400px;
	margin-right: 20px;
	border: 1px solid rgb(229, 232, 235);
	border-radius: 10px; 
	display:flex;
	align-items:center;
}

.search-area i{
	margin-right: 12px;
	font-size: 18px;
}

.search-bar{
	width:100%;
	height: 90%;
	font-size: 2.5vh;
	font-weight:bold;
	
	border: none;
	outline:transparent;
	
}

.search-bar::placeholder{
	color: rgba(0, 0, 0,0.5);
}

:-ms-input-placeholder { /* Internet Explorer 10-11 */
  color: rgba(0, 0, 0,0.5);
}

::-ms-input-placeholder { /* Microsoft Edge */
  color: rgba(0, 0, 0,0.5);
}

.pages-nav-area{
	display:flex;
	justify-content:space-between;
	margin-right: 50px;
	font-size: 16px;
	font-weight: bold;
	color: var(--sub-text-color);
}

.pages-nav-area>li{
	/* background:pink; */
	position:relative;
	display:flex;
	align-items:center;
	height:40px;
	
}

.pages-nav-area>li:hover{
	color:black;
	cursor:pointer;
}



.pages-nav-area li{
	list-style:none;
}

.nav-area-sub-menu{
	background:#fff;
	min-width:230px;
	position:absolute;
	top: 30px;
	left:0px;
	display:none;
	min-height: 50px;
}

.pages-nav-area>li:hover .nav-area-sub-menu{
	display:inline-block;
}

.nav-area-sub-menu>li{
	/* background:pink; */
	height:50px;
	width:100%;
	display:flex;
	align-items:center;
	padding:20px;
	/* margin-top: 20px; */
	
}



.nav-area-sub-menu>li:hover{
	box-shadow:var(--gen-shadow);
}

.nav-area-sub-menu>li>img{
	height:20px;
	width:20px;
	margin-right: 20px;
}

.nav-area-sub-box{
	/* background:pink; */
	width:100%;
	height:100%;
	display:flex;
	justify-content:space-between;
	align-items:center;
}

.nav-area-sub-box>i{
	font-size: 20px;
	color:var(--light-text-color);
}

.nav-icons-area{
	display:flex;
	justify-content:space-between;
	font-size: 28px;
	color: var(--sub-text-color);
}

.nav-icons-area>i:hover{
	color:#000;
	cursor:pointer;
}

		
		/* HERO SECTION */
.hero-area{
	position:relative;
	background: url('../img/volcano.jpg') -0px -520px/cover no-repeat ;
	min-height: 95vh;
	width:100%;
	
}

.frost-screen{
	position:absolute;
	top:0;
	left:0;
	height:100%;
	width:100%;
	-webkit-backdrop-filter: blur(10px);
  backdrop-filter: blur(10px);
	background:linear-gradient(rgba(255,255,255,0.7),rgba(255,255,255,0.8),rgba(255,255,255,0.9), rgba(255,255,255,0.9),rgba(255,255,255,1));
	/* background:red; */
	padding: 38px 20px 45px 20px;
	display:grid;
	grid-template-columns: 1fr 1fr;
	grid-column-gap:25px;
}

.left-hero,.right-hero{
	/* background:red; */
	width:100%;
	padding:35px;
	/* margin: 0px 35px; */
}

.left-hero{
	display:flex;
	flex-direction:column;
	padding-top: 65px;
	padding:65px 34px 80px 34px;
	/* padding-bottom: 80px; */
	justify-content: space-between;
	/* height:80%; */
	/* flex:3; */
	/* background:pink; */
}

.left-hero-btn-area{
	margin-top: 17px;
}


.left-hero>h1{
	font-size: 47.5px;
	line-height: 110%;
}

.left-hero>h2{
	font-weight: 500;
	line-height: 140%;
}

.main-buttons{
	height: 50px;
	min-width: 8vw;
	width: 16vw;
	margin-right: 20px;
	border:1px solid var(--logo-color);
	border-radius: 10px;
	font-size: 15px;
	font-weight:bold;
	cursor:pointer;
}

.main-buttons.explore{
	height: 50px;
	min-width: 18vw;
	width: 18vw;
}

.button-blue{
	background: var(--logo-color);
	color: #fff;
}

.left-hero button:last-child{
	background: #fff;
	color: var(--logo-color);
}

.right-hero{
	grid-row: span 2;
	padding: 0px 35px;
}

.hero-nft{
	/* background:yellow; */
	height:100%;
	border-radius:var(--main-border-rad);
	overflow:hidden;
	cursor:pointer;
	box-shadow: 0px 2px 2px rgba(0,0,0,0.2);
	/* display:grid; */
	/* grid-template-rows: 3fr 1fr; */
	/* display:flex; */
	/* flex-direction:column; */
}

.hero-nft>header{
	position:relative;
	height: 85%;
	/* flex:4; */
	
}

.hero-nft>header>img{
	position:absolute;
	top:0;
	height:100%;
	width:100%;
	/* max-width:100%; */
	/* min-height:100%; */
	object-fit:cover;
}

.hero-nft>footer{
	/* flex:1; */
	background:#fbfdff;
	height:15%;
	display:flex;
	justify-content:space-between;
	align-items:center;
	padding:0px 20px;
	color:black;
}

.hero-nft-profile{
	display:flex;
	align-items:center;
}

.hero-nft-profile img{
	height: 40px;
	border-radius:50%;
	margin-right: 20px;
}

.hero-nft-profile p{
	font-size: 15px;
	line-height:140%;
	font-weight:bold;
}

.hero-nft-profile p:last-child{
	color: var(--logo-color);
}

.left-hero-base{
	/* background:yellow; */
	display:flex;
	align-items: center;
	
	padding: 12px 0px 2px 35px;
	color: var(--logo-color);
}

.left-hero-base>i{
	margin-right:10px;
}

		/* NOTABLE DROPS SECTION */
.notable-drops-area{
	/* background:pink; */
	height: 140vh;
	min-height: 140vh;
	width:100%;
	padding: 0px 25px;
	/* overflow:hidden; */
}

.notable-drops-area>h2{
	height: 17%;
	display:flex;
	align-items:center;
	justify-content:center;
	font-size: 26px;
}

.carousel-case{
	position:relative;
	height:70%;
	width:100%;
	cursor:pointer;
}

.carousel-filter{
	height:100%;
	width: 95%;
	margin:auto;
	display:grid;
	grid-template-columns: repeat(3,1fr);
	grid-column-gap: 20px;
}



.carousel-post{
	background:red;
	height:100%;
	width:100%;
	border-radius:var(--main-border-rad);
	
	overflow:hidden;
}

.carousel-post:hover{
	box-shadow: var(--gen-shadow);
}

.carousel-post:nth-child(2){
	background:black;
}

.carousel-post:nth-child(3){
	background:rgba(0,0,0,0.8);
}

.carousel-post>header{
	/* background:yellow; */
	position:relative;
	height: 65%;
	/* padding: 0px 30px; */
	display:flex;
	flex-direction:column;
	justify-content:center;
	/* align-items:center; */
}

.carousel-post>header>img{
	position:absolute;
	height:100%;
	width:100%;
	object-fit:cover;
}

.carousel-post>header>h1{
	font-size: 50px;
	/* text-align:center; */
	color:red;
}

.carousel-post>header>h3{
	font-size: 20px;
	color:red;
}


.carousel-post-footer{
	height:35%;
	width:100%;
	padding: 13px 40px;
	text-align:center;
	display:flex;
	flex-direction:column;
	justify-content: space-around;
	align-items:center;
	color:#fff;
	
}

.carousel-post-footer>h5,
.carousel-post-footer>p:last-child{
	font-size: 18px;
	font-weight: bold;
}

.carousel-post-footer>p:nth-child(2){
	font-size:16px;
	margin-bottom:5px;
}

.carousel-post-footer>button{
	padding:5px 10px;
	background:transparent;
	color:white;
	border:1px solid #fff;
	border-radius: 10px;
}


.carousel-btn-case{
	position:absolute;
	width:100%;
	top:65%;
	transform: translateY(-50%);
	display:flex;
	justify-content:space-between;
	cursor:pointer;
}

.carousel-btn{
	background:#fff;
	padding:15px 19px;
	display:flex;
	align-items:center;
	justify-content:center;
	border-radius:50%;
	border:1px solid rgba(0,0,0,0.1);
	font-size: 18px;
	font-weight:bold;
	cursor:pointer;
}

.carousel-btn:hover{
	box-shadow: var(--gen-shadow);
}

.carousel-nav{
	width:100%;
	height:20px;
	margin-top:20px;
	display:flex;
	justify-content:center;
}

.carousel-nav>li{
	color: rgba(24,104,183,0.8);
	margin-left: 10px;
	cursor:pointer;
}

.carousel-nav>li:not(.carousel-nav>li:first-child){
	color:rgba(24,104,183,0.4);
}
.carousel-nav>li:not(.carousel-nav>li:first-child):hover{
	color:rgba(24,104,183,1);
}


	/* TOP COLLECTIONS SECTION */
.top-collections-section{
	/* background:pink; */
	height:115vh;
	min-height:115vh;
	padding:25px 50px;
}

.section-heading{
	width:100%;
	text-align:center;
	padding:40px 0px;
	margin-bottom:10px;
}

.section-heading>span{
	color:var(--logo-color);
	cursor:pointer;
}

.collections-filter{
	/* background:coral; */
	height:70%;
	width:100%;
	display:grid;
	grid-template-columns: repeat(3,1fr);
	grid-column-gap: 45px;
}


.collections-box{
	/* background:blue; */
	display:grid;
	grid-template-rows: repeat(5,1fr);
	/* grid-template-rows: repeat(3,1fr); */
}

.main-collection{
	/* background:green; */
	display:grid;
	grid-template-columns:0.5fr 1fr 1.5fr 3fr;
	align-items:center;
	font-weight:bold;
	padding: 0px 20px;
	height:100%;
	border-bottom:1px solid rgb(229, 232, 235);
	cursor:pointer;
	/* transition: all 0.1s ease-in; */
}

.main-collection:hover{
	border-bottom:none;
	border-radius: 12px;
	box-shadow:var(--gen-shadow);
}


.main-collection>img{
	height:48px;
	width:48px;
	border-radius:50%;
	margin-right:12px;
	margin-left:10px;
}

.user-state{
	width: 150px;
}

.user-state>p{
	/* background:pink; */
	/* max-width:150px; */
	white-space: nowrap;
    overflow: hidden;
	text-overflow: ellipsis;
}

.user-state span{
	margin-top:3px;
	display:flex;
}

.user-state span>p{
	margin-left:5px;
	font-size: 14px;
	color:rgb(138 147 155);
}

.main-collection>p:last-child{
	justify-self:flex-end;
	color:rgb(52, 199, 123);
	font-weight:300;
}

.rankings-btn-case{
	width:100%;
	padding:40px 0px;
	display:flex;
	align-items: center;
	justify-content:center;
	margin-top: 10px;
}


		/* TRENDING CATEGORIES SECTION */
.trending-category-section{
	/* background:pink; */
	width:100%;
	height: 110vh;
	padding:25px 50px;
	margin-top: 50px;
}

.categories-box{
	/* background:coral; */
	position:relative;
	height:65%;
	width:100%;
}

.categories-filter{
	/* background:pink; */
	height:100%;
	width:95%;
	margin:auto;
	display:grid;
	grid-template-columns: repeat(3,1fr);
	grid-column-gap: 20px;
}

.main-category{
	/* background:blue; */
	position:relative;
	height:100%;
	width:100%;
	border: 1px solid rgb(229, 232, 235);
	border-radius: var(--main-border-rad);
	overflow:hidden;
	cursor:pointer;
}

.main-category:hover{
	box-shadow: var(--gen-shadow);
}

.main-category>header{
	/* background:yellow; */
	position:relative;
	height:50%;
	width:100%;
}

.main-category>header>img{
	position:absolute;
	height:100%;
	width:100%;
	object-fit:cover;
}

.main-category>img{
	position:absolute;
	top:50%;
	left:50%;
	transform: translate(-50%,-50%);
	height: 40px;
	width: 40px;
	border: 4px solid #fff;
	border-radius: 50%;
}


.main-category>footer{
	background:#fff;
	height:50%;
	width:100%;
	padding: 20px 40px;
	display:flex;
	flex-direction:column;
	justify-content:space-evenly;
	align-items:center;
	color: var(--sub-text-color);
	text-align:center;
	cursor:pointer;
}

.category-footer-heading{
	display:flex;
	flex-direction:column;
	justify-content:center;
	align-items:center;
	
}

.category-footer-heading>p:first-child{
	font-weight:bold;
	color:#000;
}

.main-category>footer span{
	color: var(--logo-color);
}

.categories-btn-case{
	position:absolute;
	width:100%;
	top:50%;
	transform: translateY(-50%);
	display:flex;
	justify-content:space-between;
}

		/* CREATE AND SELL SECTION */

.create-sell-section{
	background:rgb(251 253 255);
	width:100%;
	height: 80vh;
	min-height:80vh;
	padding:25px 30px;
	margin-top: 30px;
}

.create-sell-box{
	/* background:coral; */
	width:100%;
	height: 52%;
	display:grid;
	grid-template-columns: repeat(4, 1fr);
	grid-column-gap: 60px;
}

.main-create{
	/* background:pink; */
	width:100%;
	height:100%;
	display:flex;
	flex-direction:column;
	justify-content: space-around;
	align-items:center;
	text-align:center;
}

.main-create>h6{
	font-size: 16px;
}

.main-create>p{
	font-size: 18px;
	color:var(--sub-text-color);
}

.main-create>img{
	height:40px;
}

.main-create span{
	color: var(--logo-color);
	font-weight:bold;
	cursor:pointer;
}

	/* RESOURCES SECTION */
.resources-section{
	/* background:pink; */
	width:100%;
	min-height:75vh;
	padding: 30px 40px;
}

.resources-box{
	/* background:blue; */
	position:relative;
	height: 60vh;
	/* width:100%; */
	/* margin:auto; */
	
}

.resources-filter{
	/* background:pink; */
	width:96%;
	height:100%;
	margin:auto;
	display:grid;
	grid-template-columns: repeat(3,1fr);
	grid-column-gap:20px;
}

.main-resource{
	height:100%;
	width:100%;
	border: 1px solid rgb(229 232 235);
	border-radius: var(--main-border-rad);
	overflow:hidden;
	cursor:pointer;
}

.main-resource:hover{
	box-shadow: var(--gen-shadow);
}

.main-resource>header{
	background:coral;
	position:relative;
	height:75%;
	width:100%;
	
}

.main-resource>header>img{
	position:absolute;
	top:0;
	left:0;
	width:100%;
	height:100%;
	object-fit:cover;
}

.main-resource>footer{
	background:#fff;
	width:100%;
	height:25%;
	padding: 0px 35px;
	display:flex;
	justify-content:center;
	align-items:center;
	font-weight:bold;
	line-height: 150%;
}


.categories-btn-case-resource{
	position:absolute;
	width:100%;
	top:75%;
	transform: translateY(-50%);
	display:flex;
	justify-content:space-between;
}

		/* BROWSE CATEGORY SECTION */
.browse-category-section{
	/* background:pink; */
	width:100%;
	height: 170vh;
	min-height: 150vh;
	padding:25px 30px;
	margin-top: 30px;
}

.browse-category-box{
	width:100%;
	height: 80%;
	padding:0px 100px;
	display:grid;
	grid-template-columns: repeat(3,1fr);
	grid-template-rows: repeat(3,1fr);
	gap:40px 20px;
}

.main-browse-category{
	width:100%;
	height:100%;
	border: 1px solid rgb(229 232 235);
	border-radius: var(--main-border-rad);
	overflow:hidden;
	cursor:pointer;
}

.main-browse-category:hover{
	box-shadow: var(--gen-shadow);
}

.main-browse-category>header{
	/* background:blue; */
	position:relative;
	height:80%;
	width:100%;
}

.main-browse-category>header>img{
	position:absolute;
	width:100%;
	height:100%;
	object-fit:cover;
}

.main-browse-category>footer{
	background:white;
	width:100%;
	height:20%;
	display:flex;
	align-items:center;
	justify-content:center;
}

.main-browse-category>footer>p{
	font-weight:bold;
	font-size: 18px;
	color: var(--sub-text-color);
}

	/* MEET SECTION */
.meet-section	{
	/* background:rgb(251, 253, 255); */
	background:rgb(237 246 255 / 55%);
	width:100%;
	height: 170vh;
	padding: 20px 50px;
}
	

.meet-section-headings{
	/* background:pink; */
	width:100%;
	height: 16%;
	/* text-align:center; */
	display:flex;
	flex-direction:column;
	justify-content:space-between;
	align-items:center;
	padding:30px 0px;
	margin-bottom:10px;
}

.meet-section-headings>h1{
	font-size:44px;
}

.meet-section-headings>p{
	color:var(--sub-text-color);
}

.iframe-case{
	width:80%;
	margin: auto;
	height: 58%;
	display:flex;
	justify-content:center;
}

.iframe-case>iframe{
	width: 100%;
	height:100%;
}

.meet-section>p{
	width:100%;
	text-align:center;
	margin-top: 10px;
	color: var(--sub-text-color);
	font-weight:bold;
	font-size: 14px;
}

.main-buttons.explore{
	margin-top: 50px;
}



		/* PARTNERS SECTION */
.partners-section{
	background-color: rgb(32, 129, 226);
	width:100%;
	min-height:100px;
	padding: 10px 50px;
	display:grid;
	grid-template-columns: repeat(8,1fr);
	align-items:center;
}


.partners-section>svg{
	height: 40px;
	width: 110px;
	color: rgb(32, 129, 226);
	stroke:transparent;
	fill: rgba(251, 253, 255, 0.75);
	cursor:pointer;
}

.partners-section>svg.TrustWallet{
	stroke: rgb(32, 129, 226);
	/* border: 1px solid red; */
}


		/* FOOTSECTION */
.footer-section{
	background: var(--logo-color);
	width:100%;
	height: 145vh;
	padding: 20px 100px;
	color:white;
}

.footer-top{
	/* background:pink; */
	height: 42vh;
	display:grid;
	grid-template-columns: 1fr 1fr;
	border-bottom: 1px solid rgba(251, 253, 255, 0.75);
	grid-column-gap: 140px;
	padding: 35px 0px;
}

.loop{
	display:flex;
	flex-direction:column;
	justify-content:space-between;
	
}

.loop form input[type="text"]{
	background:#fff;
	width: 25vw;
	height: 45px;
	padding: 10px 20px;
	margin-right:10px;
	border:none;
	border-radius: var(--main-border-rad);
	font-size: 17px;
}

.loop form input[type="submit"]{
	background-color: rgb(32, 129, 226);
	width: 10vw;
	height: 45px;
	padding: 10px 20px;
	border:none;
	border-radius: var(--main-border-rad);
	font-size: 17px;
	font-weight:bold;
	color:white;
}

.loop form input[type="submit"]:hover{
	background-color: rgb(74 157 239);
	cursor:pointer;
	
}

.community-btns-case{
	/* background: yellow; */
	width:100%;
	height:60px;
	display:flex;
	justify-content:space-between;
}

.community-btns-case>button{
	background-color: rgb(32, 129, 226);
	width: 60px;
	height:100%;
	border:none;
	border-radius:5px;
	margin-top:15px;
}

.community-btns-case>button>i{
	color:#fff;
	font-size: 24px;
}

.community-btns-case>button:hover{
	background-color: rgb(74 157 239);
	cursor:pointer;
}

.footer-middle{
	height: 85vh;
	width:100%;
	padding: 80px 0px;
	border-bottom: 1px solid rgba(251, 253, 255, 0.75);
	display:grid;
	grid-template-columns: 1fr 3fr;
	grid-column-gap: 130px;
}

.footer-middle-left{
	/* background: pink; */
	width:100%;
	height: 38vh;
	display:flex;
	flex-direction:column;
	justify-content:space-between;
}

.footer-middle-left>img{
	height:40px;
	width:40px;
}

.footer-middle-left>svg{
	stroke:#fff;
	fill:#fff;
}

.footer-middle-right{
	width:100%;
	display:flex;
	justify-content: space-between;
}

.footer-middle-right li{
	list-style:none;
	line-height: 210%;
	font-size: 15px;
}

.footer-middle-right li:hover{
	font-weight:bold;
	cursor:pointer;
}

.footer-middle-right h3,.account-side{
	margin-bottom: 20px;
}

.account-side{
	margin-bottom: 8vh;
}


.footer-bottom{
	/* background:pink; */
	width: 100%;
	height:15vh;
	display:flex;
	justify-content:space-between;
	align-items: center;
	font-size: 14px;
}

.footer-bottom-right{
	display:flex;
}
.footer-bottom-right>p{
	cursor:pointer;
}


.footer-bottom-right>p:first-child{
	margin-right: 20px;
}